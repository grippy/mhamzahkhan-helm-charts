{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "docker:enableMajor",
    ":prHourlyLimitNone",
    "regexManagers:dockerfileVersions"
  ],
  "prConcurrentLimit": 5,
  "automerge": true,
  "platformAutomerge": true,
  "assignAutomerge": true,
  "assignees": [
    "mhamzahkhan"
  ],
  "bumpVersion": "patch",
  "helmv3": {
    "fileMatch": ["charts/.+/Chart\\.yaml$"]
  }
}
